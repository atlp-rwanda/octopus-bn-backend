{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "nyc --reporter=html --reporter=text mocha  --require @babel/register 'src/**/**test.js' --exit",
    "lint": "eslint ./src",
    "lintFix": "eslint --fix ./src",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrateDb": "sequelize db:migrate",
    "revertMigration": "sequelize db:migrate:undo:all",
    "seedDb": "sequelize db:seed:all",
    "revertSeed": "sequelize db:seed:undo:all",
    "clean": "rm -rf build && mkdir build",
    "build-src": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-src"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "coveralls": "^3.0.9",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "mongoose": "^5.2.2",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.1",
    "nyc": "^15.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.1",
    "request": "^2.87.0",
    "sequelize": "^5.21.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
